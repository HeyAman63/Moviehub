{
  "expo": {
    "name": "MovieFlix",
    "slug": "movieflix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo2.jpeg",
    "scheme": "movieflix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0F172A",
        "foregroundImage": "./assets/images/logo_foregroun.jpg",
        "backgroundImage": "./assets/images/logo_backgroun.jpg",
        "monochromeImage": "./assets/images/logo_monochrom.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.heyaman63.movieflix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_log.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5ead4d8e-4f7c-40a5-8dd5-0f70ed3c9fcb"
      }
    },
    "owner": "heyaman63"
  }
}
